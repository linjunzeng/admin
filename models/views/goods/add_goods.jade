extends ../main
block css
	link(rel='stylesheet', href='/css/index.css')
block content
	div#app
		el-table(:data="list")
			el-table-column(prop="id" label="id")
			el-table-column(prop="name" label="name")
			el-table-column(prop="price" label="price")
block script
	script(type='text/javascript').
		new Vue({
			el: '#app',
			data: function() {
				return {
					list: !{data.list}
				}
			}
		})
